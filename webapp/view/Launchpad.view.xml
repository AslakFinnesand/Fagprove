<mvc:View
    controllerName="fagprove.controller.Launchpad"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    core:require="{
		formatter: 'fagprove/model/formatter'
	}"
>
    <Page
        title="{i18n>appTitleLaunchpad}"
        id="idLaunchpadPage"
    >
        <content>
            <GenericTile
                id="idTeeamCalendarGenericTile"
                class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                header="{i18n>tileHeader}"
                subheader="{i18n>tileSubHeader}"
                wrappingType="Normal"
                press=".onGenericTilePress"
            >
                <TileContent>
                    <ImageContent
                        src="../../webapp/image/dfo_symbol_neg_large.png"
                    />
                </TileContent>
            </GenericTile>
        </content>
    </Page>
</mvc:View>
